{
  "name": "mantine-boilerplate",
  "version": "0.0.1",
  "description": "ðŸ’™ Stable, compatible, powerful, clear and production-ready ðŸ“¦ðŸ‘Œ",
  "type": "module",
  "private": true,
  "keywords": [
    "mantine",
    "mantine boilerplate",
    "mantine starter"
  ],
  "author": "selimdoyranli",
  "contributors": [
    "Selim DoyranlÄ± <selimdoyranli@gmail.com> (https://selimdoyranli.com)"
  ],
  "bugs": "selimdoyranli@gmail.com",
  "homepage": "https://github.com/selimdoyranli/mantine-boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/selimdoyranli/mantine-boilerplate"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:eslint": "eslint \"**/*.{ts,tsx,js}\" --cache",
    "lint:eslint:fix": "eslint \"**/*.{ts,tsx,js}\" --cache --fix",
    "lint:stylelint": "stylelint '**/*.css' --cache",
    "lint:stylelint:fix": "stylelint '**/*.css' --cache --fix",
    "prettier": "prettier --check \"**/*.{ts,tsx,js,css}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx,js,css}\"",
    "vitest": "vitest run",
    "vitest:watch": "vitest",
    "test": "npm run typecheck && npm run prettier && npm run lint && npm run vitest && npm run build",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "prepare": "husky",
    "commit": "cz",
    "changelog": "changelogen"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js}": [
      "npm run lint:eslint",
      "npm run prettier:write"
    ],
    "**/*.css": [
      "npm run lint:stylelint",
      "npm run prettier:write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@mantine/core": "7.15.2",
    "@mantine/hooks": "7.15.2",
    "clsx": "^2.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.0.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.15.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
    "@storybook/addon-a11y": "^8.5.0",
    "@storybook/addon-essentials": "^8.5.0",
    "@storybook/addon-interactions": "^8.5.0",
    "@storybook/addon-links": "^8.0.0",
    "@storybook/addon-storysource": "^8.5.0",
    "@storybook/addon-themes": "^8.0.0",
    "@storybook/react": "^8.5.0",
    "@storybook/react-vite": "^8.5.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.10.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "changelogen": "^0.5.7",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.15.0",
    "eslint-config-mantine": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "esno": "^4.8.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.3.0",
    "postcss": "^8.4.49",
    "postcss-preset-mantine": "1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.4.1",
    "prop-types": "^15.8.1",
    "react-element-to-jsx-string": "^15.0.0",
    "storybook": "^8.5.0",
    "storybook-dark-mode": "^4.0.2",
    "stylelint": "^16.10.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0",
    "vite": "^6.0.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.6"
  },
  "packageManager": "yarn@4.5.3"
}
